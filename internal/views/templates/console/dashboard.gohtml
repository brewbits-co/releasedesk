{{ define "content" }}
    {{- /*gotype: github.com/brewbits-co/releasedesk/internal/controllers/product.DashboardData*/ -}}
    {{ if .SetupGuideCompleted}}
        <div class="grid justify-items-end">
            <app-platform-drawer slug="{{.CurrentProductData.ProductSlug}}" name="{{.CurrentProductData.ProductName}}"></app-platform-drawer>
        </div>
        <div class="pb-5">
            <h3 class="text-xl font-semibold tracking-tight text-gray-900">Latest Releases</h3>
        </div>
        <div class="rounded-lg bg-white shadow p-4">
            <h1>oko</h1>
        </div>

        <div class="pb-5 pt-5">
            <h3 class="text-xl font-semibold tracking-tight text-gray-900">Latest Builds</h3>
        </div>
        <div class="rounded-lg bg-white shadow p-4">
            <h1>oko</h1>
        </div>
    {{else}}
        <div class="pb-5">
            <h3 class="text-xl font-semibold tracking-tight text-gray-900">Congrats on the new product!</h3>
            <p class="mt-2 max-w-4xl text-sm text-gray-500">
                Complete these simple steps to get your software distribution up and running.</p>
        </div>
        <div class="rounded-lg bg-white shadow">
            <product-setup-guide slug="{{.CurrentProductData.ProductSlug}}"></product-setup-guide>
        </div>
    {{end}}
{{ end }}
